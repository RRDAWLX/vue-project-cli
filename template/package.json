{
  "name": "vue-template",
  "version": "0.1.0",
  "description": "vue template",
  "private": true,
  "scripts": {
    "dev": "vp dev",
    "check-verson": "vp check-version",
    "lint": "eslint src/"
  },
  "pre-commit": [
    "check-version"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RRDAWLX/vue-boilerplate.git"
  },
  "keywords": [
    "vue",
    "template"
  ],
  "author": "rrdawlx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RRDAWLX/vue-boilerplate/issues"
  },
  "homepage": "https://github.com/RRDAWLX/vue-boilerplate#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.9.0",
    "eslint-plugin-html": "^4.0.6"
  },
  "babel": {
    "presets": [
      "vue",
      [
        "env",
        {
          "useBuiltIns": true
        }
      ],
      "stage-3"
    ],
    "plugins": [
      "syntax-dynamic-import"
    ],
    "compact": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
